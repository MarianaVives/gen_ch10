package java01;

public class HolaMundo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.println("Hola Mundo desde el IDE");
		//Definimos una variable boolean
		boolean result = true;
		//Definimos una variable de tipo char
		char capitalC = 'C';
		//Definimos una variable de tipo byte
		byte b = 100;
		//Definimos una variable de tipo shart
		short s = 10000;
		//Definimos una variable de tipo int
		int i = 1000000;
		System.out.println(result);
		System.out.println(capitalC);
		System.out.println(b);
		System.out.println(s);
		System.out.println(i);
	}

}
